{
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitAny": true,
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "lib": [
      "es2017",
      "es7",
      "es6",
      "dom"
    ],
    "declaration": true,
    "strict": true,
  }
}